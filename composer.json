{
  "name": "videocast/php-sdk",
  "type": "library",
  "description": "VideoCast PHP SDK for REST APIs",
  "keywords": [
    "videocast",
    "rest",
    "sdk"
  ],
  "homepage": "http://dev.videocast.com.br",
  "license": "MIT",
  "authors": [
    {
      "name": "Welsiton Ferreira",
      "email": "wfcreations@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "VideoCast\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VideoCast\\Tests\\": "tests/"
    }
  }
}