{
  "name": "videocast/php-sdk",
  "type": "library",
  "description": "VideoCast PHP SDK for REST APIs",
  "keywords": [
    "videocast",
    "rest",
    "sdk"
  ],
  "homepage": "http://developers.videocast.com.br",
  "license": "MIT",
  "authors": [
    {
      "name": "Welsiton Ferreira",
      "email": "wfcreations@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/videocast/php-sdk/issues"
  },
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.2.2",
    "guzzlehttp/psr7": "~1.3.1",
    "guzzlehttp/promises": "~1.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "VideoCast\\": "lib"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "VideoCast\\Tests\\": "tests/"
    }
  }
}